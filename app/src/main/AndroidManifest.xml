<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.isabella.uncommonbooks" >

    <uses-permission android:name="android.permission.INTERNET" >
</uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" >
    </uses-permission>

    <uses-library android:name="com.google.android.books" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MyActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DetailActivity"
            android:label="Detail Activity" >

            <!--
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            -->
        </activity>
        <activity
            android:name=".SearchResultsActivity"
            android:label="@string/search_results_title" >
        </activity>
        <activity
            android:name=".ListOfListsActivity"
            android:label="@string/search_results_title" >
        </activity>
        <activity
            android:name=".ListDetailActivity"
            android:label="@string/search_results_title" >
        </activity>

    </application>

</manifest>
